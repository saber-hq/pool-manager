{
  "name": "@saber-hq/pool-manager",
  "version": "0.1.7",
  "description": "Admin proxy for Saber pools.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "keywords": [
    "solana",
    "blockchain"
  ],
  "bugs": {
    "url": "https://github.com/saber-hq/saber-pool-manager/issues",
    "email": "ian@ship.capital"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saber-hq/saber-pool-manager.git"
  },
  "author": "Saber Team <team@saber.so>",
  "homepage": "https://saber.so",
  "license": "AGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@project-serum/anchor": "^0.20.1",
    "@rushstack/eslint-patch": "^1.1.0",
    "@saberhq/anchor-contrib": "^1.12.32",
    "@saberhq/chai-solana": "^1.12.32",
    "@saberhq/eslint-config": "^1.12.32",
    "@saberhq/solana-contrib": "^1.12.32",
    "@saberhq/stableswap-sdk": "^1.12.32",
    "@saberhq/token-utils": "^1.12.32",
    "@saberhq/tsconfig": "^1.12.32",
    "@solana/web3.js": "^1.31.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.0",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/lodash.zip": "^4.2.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.10",
    "@yarnpkg/doctor": "^3.1.0",
    "bn.js": "^5.2.0",
    "chai": "^4.3.4",
    "eslint": "^8.7.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jsbi": "^4.1.0",
    "lint-staged": "^12.1.7",
    "lodash.mapvalues": "^4.6.0",
    "mocha": "^9.1.4",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,json,html,css,md}": "prettier --write",
    "*.nix": "nixfmt"
  },
  "scripts": {
    "build": "tsc -P tsconfig.build.json && tsc -P tsconfig.esm.json",
    "typecheck": "tsc",
    "lint": "eslint . --cache",
    "test": "anchor test --skip-build 'tests/*.spec.ts'",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts",
    "prepare": "husky install",
    "idl:generate": "./scripts/idl.sh && ./scripts/generate-idl-types.sh",
    "idl:generate:nolint": "./scripts/idl.sh && RUN_ESLINT=none ./scripts/generate-idl-types.sh"
  },
  "dependencies": {
    "lodash.mapvalues": "^4.6.0",
    "lodash.zip": "^4.2.0",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@project-serum/anchor": "^0.19 || ^0.20",
    "@saberhq/anchor-contrib": "^1.12",
    "@saberhq/solana-contrib": "^1.12",
    "@saberhq/stableswap-sdk": "^1.12",
    "@saberhq/token-utils": "^1.12",
    "@solana/web3.js": "^1.29.2",
    "bn.js": "^5.2.0",
    "jsbi": "^3 || ^4"
  },
  "packageManager": "yarn@3.1.1",
  "files": [
    "dist/",
    "src/"
  ]
}
